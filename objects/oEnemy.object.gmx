    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if active {
    //////////////////////////
    // misc updates
    //////////////////////////

    objectAge += 1;
    //if HP &lt; 0 kill(self);
    if killWhenBelowScreen {
       if y &gt; (vh + 64*scale) {
        value = 0;
        kill(self);
       }
    }

    if killWhenOffscreen {
        if offscreen() {
            if offScreenTimer &lt;= 0 {
                value = 0;
                kill(self);
            }
            offScreenTimer -= 1;
        } else {
            offScreenTimer = 36;
        }
    }

    if invul and invulTime &gt; -1 {
        invulTime -= 1;
        if invulTime &lt; 0 invul = false;
    }

    if recoil &gt; 0 {
        recoil -= 4;
        y+=4;
    }

    //////////////////////////
    // handle paths
    //////////////////////////

    if path_index == -1 {
        if path &gt; -1 {
            //trace('enemy starting path at spd '+string(path_speed));
            path_start(path,path_speed,path_endaction,false);
        }
    } else {
        if (path_endtime &gt; -1 and objectAge == path_endtime) {
            path = -1;
            path_end();
            if killWhenPathComplete kill();
        }
    }
    
    //////////////////////////
    // handle bezier curve
    //////////////////////////

    if followBezier {
        var bp;
        bp = Bezier_Point_Find(bezierPos,bezierID.cp0.x,bezierID.cp0.y,bezierID.cp1.x,bezierID.cp1.y,bezierID.cp2.x,bezierID.cp2.y,bezierID.cp3.x,bezierID.cp3.y);

        var speed_per_tick = bezierSpeedSign*bezierSpeed; //constant speed you want the object to move at

        if objectAge == 1 {
            x = bp[0];
            y = bp[1];
        } else {
            x_goal = bp[0];
            y_goal = bp[1];

            if bezierSpeedSign &gt; 0 {
                delta_x = x_goal - x;
                delta_y = y_goal - y;
            } else {
                delta_x = x - x_goal;
                delta_y = y - y_goal;
            }
            //trace('delta x:'+string(delta_x));

            goal_dist = sqrt( (delta_x * delta_x) + (delta_y * delta_y) )
            //trace('goal dist: '+string(goal_dist)+','+string(speed_per_tick));

            if (abs(goal_dist) &gt; abs(speed_per_tick)) {
                // move twd goal
                ratio = speed_per_tick / goal_dist
                x_move = ratio * delta_x  
                y_move = ratio * delta_y
                x = x_move + x;
                y = y_move + y;
            } else {
                // reached goal, advance position
                //trace('*** reached goal position');
                x = x_goal;
                y = y_goal;

                // if at the end of the path..

                if bezierPos &gt; 1 or bezierPos &lt; 0 {
                    //trace('*** reached end of path');
                    bp = Bezier_Point_Find(bezierPos,bezierID.cp0.x,bezierID.cp0.y,bezierID.cp1.x,bezierID.cp1.y,bezierID.cp2.x,bezierID.cp2.y,bezierID.cp3.x,bezierID.cp3.y);
                    bezierSpeedSign *= -1;

                    /*
                    bezierPos = 0;
                    bp = Bezier_Point_Find(bezierPos,bezierID.cp0.x,bezierID.cp0.y,bezierID.cp1.x,bezierID.cp1.y,bezierID.cp2.x,bezierID.cp2.y,bezierID.cp3.x,bezierID.cp3.y);
                    x = bp[0];
                    y = bp[1];
                    */
                }

                bezierPos += bezierSpeedSign*.1;
            }


        }
        /*
        bp = Bezier_Point_Find(bezierPos,bezierID.cp0.x,bezierID.cp0.y,bezierID.cp1.x,bezierID.cp1.y,bezierID.cp2.x,bezierID.cp2.y,bezierID.cp3.x,bezierID.cp3.y);
        x = bp[0];
        y = bp[1];
        bezierPos += .01;
        if bezierPos &gt; 1 bezierPos = 0;
        //direction = point_direction(x,y,bp[0],bp[1]);
        //speed = 6;
        */
    }
    
    //////////////////////////
    // run phase
    //////////////////////////

    enemy_RunPhase(currentPhase);

} else {
    if waitTime &gt;= 0 {
        waitTime -= 1;
    } else {
        if alarm[0] == -1 {
            alarm[0] = 1;
        }
        //active = true;
    }
}
</string>
