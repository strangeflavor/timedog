        <arguments>
          <argument>
            <kind>1</kind>
            <string>if active {
    if _xo != 0 and instance_number(oMirror) == 1 _xo = 0;

    if absorb {
        absorbCount += 1;

        _sparkdirection = irandom(360);
        _sparkx = lengthdir_x(sprite_width/2+32,_sparkdirection);
        _sparky = lengthdir_y(sprite_height/2+32,_sparkdirection);
        repeat 2 {
            with make(x+_sparkx,y+_sparky,oMirrorFX) {
                parentID = other.id;
                image_angle = irandom(360);
                sprite_index = sSparkle;
                image_speed = choose(.2,.3);
                image_scale(2);
                depth = other.depth - 1;
                //direction = other._sparkdirection - 180;
                //speed = 7;
                //friction = .02;
            }
        }
        image_scale(wave(2,2.5,.666,0));
    }

    image_scale(lerp(image_xscale,2,.05));
    absorb = false;
    if absorbCount &gt; 17 {
        change_state(_ATK_BEAMS);
        absorbCount = -1;
    }

    switch state {
        case _APPROACH:
            x = Ease(x,oShip.x+_xo,.15,EaseInBounce);
            y = Ease(max(20,oShip.y-bh*.25),y,.07,EaseInQuad);

            image_speed = .5;
        break;
        case _FOLLOW:

            if instance_exists(oShip) {
                //if instance_number(oMirror) &gt;= 2 _xo = xo else _xo = 0;

                x = lerp(x,oShip.x+_xo,.2);
                y = lerp(max(20,oShip.y-bh*.25),y,.7);

                if x &lt; oShip.x - 8 {
                    image_angle = lerp(-15,image_angle,.7);
                } else {
                    if x &gt; oShip.x + 8 {
                        image_angle = lerp(15,image_angle,.7);
                    } else {
                        image_angle = lerp(0,image_angle,.7);
                    }
                }
            }

            image_speed = wave(.75,.1,2,0);
        break;
        // fire beam
        case _ATK_BEAMS:
            if current_state_age &lt; 45 {
                x = lerp(x,oShip.x,.02) + _xo;// + irandom(16)-8;
                y = lerp(y,max(20,oShip.y-bh*.25),.001);// + irandom(16)-8;
                if current_state_age == 0 TweenSimpleRotate(0,360,44,EaseOutBounce);
            } else {
                if current_state_age == 50 {
                    with make(oBeam) {
                        xo = 0;
                        yo = 8;
                        owner = other.id;
                        duration = 120;
                        //if other.aimAtPlayer duration = 90 else duration = 120;
                    }
                } else {
                    if current_state_age &lt; 60 {
                        x = lerp(x,oShip.x,.008) + _xo;
                    } else {
                        if current_state_age &lt; 90 {
                            x = lerp(x,oShip.x,.0025) + _xo;
                        }
                    }
                }
            }

            image_speed = 1;

            if current_state_age == 90+120 {
                state = _MOVETOWARD;
            }
        break;
        case _MOVETOWARD:
            xVel = 10;
            yVel = 10;
        break;
    }
}
</string>
