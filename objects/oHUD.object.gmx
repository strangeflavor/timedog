            <string>if active {
    if showHud {
        /*
        l = 8*scale;
        repeat lives {
            //draw_sprite(sShip,0,(14*scale)+l,(24*scale));
            draw_sprite_ext(sShip,0,(14*scale)+l,(24*scale),scale,scale,0,c_white,1);
