        */

        var ttime = levelManager.alarm[1] div 60
        var minutes = ttime div 60;
        var seconds = ttime % 60;
        var frames = (levelManager.alarm[1] % 60) % 60;
        var subframes = '.'+string(irandom(9))+string(irandom(9));

        if frames &lt; 10 {
            frames = string(frames);
            frames = '0'+frames;
        } else {
            frames = string(frames);
        }

        ttime = string(minutes)+':'+string(seconds)+':'+string(frames);

        draw_set_halign(fa_right);
        draw_set_valign(fa_bottom);
        draw_text_ext(vw-32,24,ttime,-1,-1);
        
        draw_set_halign(fa_left);
        draw_text_ext_transformed(vw-32,24,subframes,-1,-1,.5,.5,0);
    }

    if showDialog {
        showHud = false;

        dIndex += dSpeed*dFrameDuration[d,floor(dIndex)];

        //draw_sprite(dSprite[d],ceil(dIndex-1),0,room_height-128-4);
        //draw_sprite(dSprite[d],ceil(dIndex-1),0,-38);
        //draw_sprite(dSprite[d],floor(dIndex),0,-38);
        draw_sprite_ext(dSprite[d],floor(dIndex),0,-38*scale,scale,scale,0,c_white,1);

        if dFrameDuration[d,floor(dIndex)] &lt; 0 {
            showDialog = false;
            showHud = true;

            // prepare for next dialog, if any
            dIndex = 0;
            dSpeed = .005
            d+=1;

            // restart timeline, if necessary
            with controller timeline_speed = 1;
        }
    }
}
</string>
