/// form_meteors(size,rate);
trace('starting form meteors');

var arg;
for (var i = 0; i < 16; i += 1;) if argument_count > i arg[i] = argument[i] else arg[i] = -1;

var size = arg[0];
var rate = arg[1];
var rateDelta = arg[2];

if size == -1 size = 12;
if rate == -1 rate = 60;
if rateDelta == 1 rateDelta = 1.25;

var mID = make(fMeteors);
